{
    "[dockercompose]": {
        "editor.autoIndent": "advanced",
        "editor.defaultFormatter": "redhat.vscode-yaml",
        "editor.insertSpaces": true,
        "editor.quickSuggestions": {
            "comments": false,
            "other": true,
            "strings": true
        },
        "editor.tabSize": 2
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "database-client.autoSync": true,
    "editor.accessibilitySupport": "off",
    "editor.fontSize": 15,
    "editor.formatOnSave": true,
    "editor.minimap.enabled": true,
    "explorer.openEditors.minVisible": 10,
    "explorer.openEditors.visible": 10,
    "files.associations": {
        "**/templates/*.yaml": "helm",
        "**/templates/*.yml": "helm",
        "**/templates/_helpers.tpl": "helm"
    },
    "files.autoSave": "onFocusChange",
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "gitlens.views.scm.grouped.views": {
        "branches": true,
        "commits": true,
        "contributors": true,
        "fileHistory": true,
        "launchpad": false,
        "remotes": true,
        "repositories": true,
        "searchAndCompare": false,
        "stashes": true,
        "tags": true,
        "worktrees": true
    },
    "redhat.telemetry.enabled": true,
    "terminal.external.osxExec": "iTerm2.app",
    "terminal.integrated.env.osx": {
        "Q_NEW_SESSION": "1"
    },
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "terminal.integrated.fontSize": 14.5,
    "terminal.integrated.hideOnLastClosed": false,
    "terminal.integrated.persistentSessionScrollback": 1000,
    "terminal.integrated.scrollback": 3000,
    "update.releaseTrack": "prerelease",
    "vs-kubernetes": {
        "vs-kubernetes.crd-code-completion": "enabled",
        "vscode-kubernetes.helm-path-mac": "/Users/changhwan/.vs-kubernetes/tools/helm/darwin-arm64/helm",
        "vscode-kubernetes.kubectl-path-mac": "/Users/changhwan/.vs-kubernetes/tools/kubectl/kubectl",
        "vscode-kubernetes.minikube-path-mac": "/Users/changhwan/.vs-kubernetes/tools/minikube/darwin-arm64/minikube"
    },
    "window.commandCenter": true,
    "workbench.activityBar.orientation": "vertical",
    "workbench.colorTheme": "One Dark Modern Pro",
    "workbench.iconTheme": "vscode-great-icons",
    "cursor.composer.textSizeScale": 1.15
}