{
    "[dockercompose]": {
        "editor.autoIndent": "advanced",
        "editor.defaultFormatter": "redhat.vscode-yaml",
        "editor.insertSpaces": true,
        "editor.quickSuggestions": {
            "comments": false,
            "other": true,
            "strings": true
        },
        "editor.tabSize": 2
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "database-client.autoSync": true,
    "editor.accessibilitySupport": "off",
    "editor.formatOnSave": true,
    "editor.minimap.enabled": true,
    "explorer.openEditors.minVisible": 10,
    "explorer.openEditors.visible": 10,
    "files.associations": {
        "**/templates/*.yaml": "helm",
        "**/templates/*.yml": "helm",
        "**/templates/_helpers.tpl": "helm"
    },
    "files.autoSave": "onFocusChange",
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "gitlens.views.scm.grouped.views": {
        "branches": true,
        "commits": true,
        "contributors": true,
        "fileHistory": true,
        "launchpad": false,
        "remotes": true,
        "repositories": true,
        "searchAndCompare": false,
        "stashes": true,
        "tags": true,
        "worktrees": true
    },
    "redhat.telemetry.enabled": true,
    "terminal.external.osxExec": "iTerm2.app",
    "terminal.integrated.env.osx": {
        "Q_NEW_SESSION": "1"
    },
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.hideOnLastClosed": false,
    "terminal.integrated.persistentSessionScrollback": 1000,
    "terminal.integrated.scrollback": 3000,
    "update.releaseTrack": "prerelease",
    "vs-kubernetes": {
        "vs-kubernetes.crd-code-completion": "enabled",
        "vscode-kubernetes.helm-path-mac": "/Users/changhwan/.vs-kubernetes/tools/helm/darwin-arm64/helm",
        "vscode-kubernetes.kubectl-path-mac": "/Users/changhwan/.vs-kubernetes/tools/kubectl/kubectl",
        "vscode-kubernetes.minikube-path-mac": "/Users/changhwan/.vs-kubernetes/tools/minikube/darwin-arm64/minikube"
    },
    "window.commandCenter": true,
    "workbench.activityBar.orientation": "vertical",
    "workbench.iconTheme": "vscode-great-icons",
    "window.zoomLevel": -0.5,
    "indentRainbow.indicatorStyle": "light",
    "indentRainbow.colors": [
        "rgba(255,255,64,0.30)",
        "rgba(127,255,127,0.30)",
        "rgba(255,127,255,0.30)",
        "rgba(79,236,236,0.30)"
    ],
    "editor.mouseWheelZoom": true,
    "workbench.editor.editorActionsLocation": "titleBar",
    "workbench.activityBar.location": "top",
    "editor.tokenColorCustomizations": {
        "[Cursor Dark]": {
            "textMateRules": [
                {
                    "scope": "support.type.property-name.json",
                    "settings": {
                        "foreground": "#D4A574"
                    }
                },
                {
                    "scope": "string.quoted.double.json",
                    "settings": {
                        "foreground": "#98C379"
                    }
                },
                {
                    "scope": "constant.numeric.json",
                    "settings": {
                        "foreground": "#56B6C2"
                    }
                },
                {
                    "scope": "constant.language.json",
                    "settings": {
                        "foreground": "#56B6C2"
                    }
                },
                {
                    "scope": "entity.name.tag.yaml",
                    "settings": {
                        "foreground": "#E06C75"
                    }
                },
                {
                    "scope": "string.unquoted.yaml",
                    "settings": {
                        "foreground": "#98C379"
                    }
                },
                {
                    "scope": "string.unquoted.plain.out.yaml",
                    "settings": {
                        "foreground": "#98C379"
                    }
                },
                {
                    "scope": "string.quoted.double.yaml",
                    "settings": {
                        "foreground": "#98C379"
                    }
                },
                {
                    "scope": "string.quoted.single.yaml",
                    "settings": {
                        "foreground": "#98C379"
                    }
                },
                {
                    "scope": "constant.numeric.yaml",
                    "settings": {
                        "foreground": "#56B6C2"
                    }
                }
            ]
        }
    },
    "workbench.colorTheme": "Cursor Dark",
    "editor.fontSize": 14
}